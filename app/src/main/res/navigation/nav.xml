<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/loginpage">
    <fragment
        android:id="@+id/loginpage"
        android:name="com.example.kauhelper.loginpage"
        android:label="fragment_loginpage"
        tools:layout="@layout/fragment_loginpage" >
        <action
            android:id="@+id/action_loginpage_to_timetable"
            app:destination="@id/timetable" />
    </fragment>
    <fragment
        android:id="@+id/timetable"
        android:name="com.example.kauhelper.timetable"
        android:label="fragment_timetable"
        tools:layout="@layout/fragment_timetable" >
        <action
            android:id="@+id/action_timetable_to_transportation"
            app:destination="@id/transportation" />
        <action
            android:id="@+id/action_timetable_to_map"
            app:destination="@id/map" />
        <action
            android:id="@+id/action_timetable_to_calendar"
            app:destination="@id/calendar" />
        <action
            android:id="@+id/action_timetable_to_menu"
            app:destination="@id/menu" />
        <action
            android:id="@+id/action_timetable_to_mypage"
            app:destination="@id/mypage" />
    </fragment>
    <fragment
        android:id="@+id/transportation"
        android:name="com.example.kauhelper.transportation"
        android:label="fragment_transportation"
        tools:layout="@layout/fragment_transportation" />
    <fragment
        android:id="@+id/map"
        android:name="com.example.kauhelper.map"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/calendar"
        android:name="com.example.kauhelper.calendar"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />
    <fragment
        android:id="@+id/menu"
        android:name="com.example.kauhelper.menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
    <fragment
        android:id="@+id/mypage"
        android:name="com.example.kauhelper.mypage"
        android:label="fragment_mypage"
        tools:layout="@layout/fragment_mypage" />
</navigation>